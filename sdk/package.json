{
  "name": "@x402/sdk",
  "version": "1.0.0",
  "description": "Professional AI Payment Infrastructure SDK - Embedded wallet system for seamless AI service payments",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./frontend": {
      "import": "./frontend/dist/index.esm.js",
      "require": "./frontend/dist/index.js",
      "types": "./frontend/dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:sdk && npm run build:frontend",
    "build:sdk": "rollup -c rollup.config.js",
    "build:frontend": "cd frontend && npm run build",
    "dev": "ts-node src/index.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts frontend/src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.ts frontend/src/**/*.{ts,tsx} --fix",
    "clean": "rimraf dist frontend/dist",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "start:examples": "concurrently \"npm run start:facilitator\" \"npm run start:server\" \"npm run start:client\"",
    "start:facilitator": "ts-node examples/standalone-facilitator.ts",
    "start:server": "ts-node examples/standalone-server.ts",
    "start:client": "ts-node examples/standalone-client.ts",
    "example:ai-server": "ts-node examples/ai-facilitator-server.ts",
    "example:ai-client": "ts-node examples/ai-facilitator-client.ts"
  },
  "keywords": [
    "x402",
    "micropayments",
    "blockchain",
    "ethereum",
    "base",
    "somnia",
    "http-402",
    "payment-system",
    "self-sovereign",
    "typescript"
  ],
  "author": "X402 Team",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "ethers": "^6.8.1",
    "express": "^5.1.0",
    "viem": "^1.19.9",
    "wagmi": "^1.4.12",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.9",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.23",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.10",
    "rollup": "^4.52.5",
    "rollup-plugin-dts": "^6.2.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/x402/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/x402/sdk/issues"
  },
  "homepage": "https://github.com/x402/sdk#readme"
}
