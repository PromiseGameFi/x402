{
  "name": "@x402/sdk",
  "version": "1.0.0",
  "description": "Plug-and-play SDK for machine-to-machine payments on the open web using the X402 protocol",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "x402",
    "payments",
    "blockchain",
    "http",
    "micropayments",
    "web3",
    "ethereum",
    "somnia",
    "sdk"
  ],
  "author": "X402 Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/coinbase/x402.git",
    "directory": "sdk"
  },
  "bugs": {
    "url": "https://github.com/coinbase/x402/issues"
  },
  "homepage": "https://x402.org",
  "dependencies": {
    "ethers": "^6.8.1",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "rollup": "^4.6.1",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "rollup-plugin-dts": "^6.1.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}